VALUES ('ANUJ',1357924680,9988776655);";
$create .="INSERT INTO Member(name,code,mobilenum)
VALUES ('RAJ',1234567890,9845879999);";

if ($conn->multi_query($create) === TRUE) {
    echo "New records created successfully";
  } else {
    echo "Error: " . $create . "<br>" . $conn->error;
